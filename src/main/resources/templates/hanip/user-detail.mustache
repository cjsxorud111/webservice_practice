{{>layout/header}}
    <br>
<h1><a href="/hanip">&nbsp;&nbsp;&nbsp;&nbsp;한입</a></h1>
    <br>
    <div class="col-md-12">
        <!-- 로그인 기능 영역 -->
        <div class="row">
            <div class="col-md-6">

                {{#userName}}
                    Logged in as: <span id="user">{{userName}}</span>
                <a href="/logout" class="btn btn-info active" role="button">Logout</a>
                {{/userName}}
                {{^userName}}
                    <a href="/login" class="btn btn-success active" role="button">Login</a>
                {{/userName}}

            </div>
        </div>
    </div>
    <br>
    <!--예약 출력 영영 -->
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>예약번호</th>
            <th>프로그램제목</th>
            <th>여행예정일</th>
            <th>예약취소</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#reservation}}
            <tr>
                <td>{{id}}</td>
                <td>{{program.title}}</td>
                <td>{{date}}</td>
                <td><a href="/user/detail" class="btn btn-info active" role="button">삭제</a></td>
            </tr>
        {{/reservation}}
        </tbody>
    </table>
    </div>

    <div>
        문의내역
        <div>
            문의수정
        </div>
    </div>

{{>layout/footer}}