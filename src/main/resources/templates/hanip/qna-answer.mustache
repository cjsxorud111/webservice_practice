{{>layout/header}}
{{>layout/banner}}
<h1>qna답변하기</h1>

<div class="col-md-12">
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label for="title">글번호</label>
                <div>{{qna.id}}</div>
            </div>
            <div class="form-group">
                <label for="title">제목</label>
                <div>{{qna.title}}</div>
            </div>
            <div class="form-group">
                <label for="title">작성자</label>
                <div>{{qna.author}}</div>
            </div>
            <div class="form-group">
                <label for="content">내용</label>
                <div>{{qna.content}}</div>
            </div>
            <div class="form-group">
                <label for="content">답변내용</label>
                <textarea class="form-control" id="content">
                    {{qna.content}}
                </textarea>
            </div>

        </form>
        <a href="/qna/detail/{{qna.id}}" role="button" class="btn btn-secondary">취소</a>
        <button type="button" class="btn btn-primary" id="btn-qna-anser">답변완료</button>
    </div>
</div>

{{>layout/footer}}